{
    "branches": [
        "teststaging"
    ],
    "plugins": [
        [
            "@semantic-release/commit-analyzer",
            {
                "preset": "angular",
                "releaseRules": [
                    {
                        "type": "chore",
                        "scope": "deps",
                        "subject": "*rollup*",
                        "release": "patch"
                    },
                    {
                        "type": "docs",
                        "scope": "README",
                        "release": "patch"
                    },
                    {
                        "type": "chore",
                        "scope": "deps",
                        "message": "*lock file maintenance*",
                        "release": "patch"
                    }
                ]
            }
        ],
        "@semantic-release/release-notes-generator",
        "@semantic-release/changelog"
    ]
}
